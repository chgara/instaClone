{
	"expo": {
		"name": "instaClone",
		"slug": "instaClone",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#fff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"buildNumber": "0.1.0",
			"bundleIdentifier": "com.chgara.instaclone"
		},
		"android": {
			"jsEngine": "hermes",
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFF"
			},
			"package": "com.chgara.instaclone",
			"versionCode": 0.1
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"description": "This is an instagram clone"
	}
}
